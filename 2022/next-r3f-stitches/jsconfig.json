{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@/*": [
				"./*"
			],
			"~/*": [
				"./*"
			],
		},
		"allowJs": true,
		// "allowSyntheticDefaultImports": true,
		// "esModuleInterop": true,
		// "forceConsistentCasingInFileNames": true,
		// "incremental": true,
		// "isolatedModules": true,
		"jsx": "react",
		"module": "commonjs",
		// "noEmit": true,
		// "resolveJsonModule": true,
		// "skipLibCheck": true,
		// "sourceMap": true,
		"strict": true,
		"strictNullChecks": true,
		"target": "es5",
	},
	"include": [
		"**/*",
		"**/*.json"
	],
	"exclude": [
		"out",
		".git",
		".app-cache",
		".npm",
		".npm-tmp",
		"dist",
		"dist*",
		"node_modules",
		"subfolder/dist",
		"subfolder/node_modules",
		"**/dist/*",
		"**/node_modules/*",
		".next",
		".vscode",
	],
}